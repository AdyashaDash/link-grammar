%
% Kazakh dictionary -- experimental -- 2016 -- Tatiana Batura, Aigerim Bakiyeva, Aigerim Yerimbetova 
%
% Dictionary version number is 5.3.0 (formatted as V5v3v0+)
<dictionary-version-number>: V5v3v0+;
% The empty word.
EMPTY-WORD.zzz: ZZZ-;

мен.pron: S1s+;
сен.pron: S2s+;
сіз.pron: Sr2s+;
ол.pron: S3s+;

біз.pron: S1p+;
сендер.pron: S2p+;
сіздер.pron: Sr2p+;
олар.pron: S3p+;

менің.pron: OF1+;
сенің.pron: OF2+;
сіздің.pron: OFr2+;
оның.pron: OF3+;

біздің.pron: OF1p+;
сендердің.pron: OF2p+;
сіздердің.pron: OFr2p+;
олардың.pron: OF3p+;

%adjectives beautiful white red
әдемі.= ақ.= қызыл.=: {AO+} & {EI-};

%nouns human girl boy dog cat
адам.= қыз.= бала.= ит.= мысық.= алма.= гүл.=: {@AO-} & {Np+} & ({S3s+} & {N+});

=.n: {N-} & {S1s+} & {S2s+} & {S3s+}; %Nominative and null suffix 

% plural suffixes 
=тар.= =тер.= =дар.= =дер.= =лар.= =лер.=: {Np-} & {Na+} & {Ng+} & ({S3p+} & {N+});


% Genitive .ng  (ілік септік, родительный падеж)
=ның.ng =нің.ng =дың.ng =дің.ng =тың.ng =тің.ng: {Ng-} & {OF3+} & {J+};

% Dative .nd (барыс септік, дательный падеж)
=ға.nd =ге.nd =қа.nd =ке.nd =на.nd =не.nd =а.nd =е.nd: {Nd-} & {OV+};
% & {Np-}  если добавить, начинаются противоречия

% Accusative .na (табыс септік, винительный падеж)
%=ды.na =ді.na 
=ты.na =ті.na =ны.na =ні.na =н.na: {Na-} & {OV+};

% Locative .nl (жатыс септік, местный/предложный падеж)
=да.nl =де.nl =та.nl =те.nl =нда.nl =нде.nl: {Nl-} & {J+};

% Ablative .nb (шығыс септік, исходный падеж)
=дан.nb =ден.nb =тан.nb =тен.nb =нан.nb =нен.nb: {Nb-};

% Instrumental .ni (көмектес септік, творительный падеж)
=бен.ni =пен.ni =мен.ni: {Ni-};



%postpositions 
артынан артында алдында алдынан: {OJV+} & {J-};


% verbs
же.= бол.= қу.= кел.=: {OJV-} & {OV-};


% suffixes for verbs (past present future, there are 7 tenses)
%субъективное прошедшее
=ыпты.vas =іпті.vas: {Vas-};

%результативное прошедшее
=қан.var =ған.var =кен.var ген.var: {Var-};

%категорическое прошедшее
=ды.vac =ді.vac =ты.vac =ті.vac: {S3s-}; %& {Vac-};
=дым.vac =дім.vac: {S1s-};

%конкретное настоящее
=п.vr =ып.vr =іп.vr =а.vr =е.vr: {Vr-};

%переходное будущее
=ады.vft =еді.vft: {Vft-};

%предположительное будущее
=ар.vfs =ер.vfs: {Vfs-};

%целенаправленное будущее
=мақ.vfg =мек.vfg =пақ.vfg =пек.vfg: {Vfg-};


=ым =ім: {OF1-};



%punctuations
",": (SJl- & SJr+ & SJl+ )
   or (AJl- & AJr+ & AJl+)
   or (VJl- & VJr+ & VJl+) ;
%",": Xc- & Xd+;


LEFT-WALL: W+ or Q+;

